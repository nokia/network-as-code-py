{
  "openapi": "3.0.2",
  "info": {
    "title": "QoS",
    "description": "QoS manages communication bandwidth for a given device.",
    "version": "0.1.0"
  },
  "paths": {
    "/sessions": {
      "post": {
        "tags": [
          "qos"
        ],
        "summary": "Create QoS service",
        "description": "Create device communication bandwidth (QoS) service.",
        "operationId": "send_subscribe_sessions_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/QoSResource"
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AsSessionWithQoSSubscription"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "RapidApiKey": {
        "type": "apiKey",
        "in": "header",
        "name": "X-RapidAPI-Key"
      }
    },
    "schemas": {
      "AsSessionWithQoSSubscription": {
        "title": "AsSessionWithQoSSubscription",
        "required": [
          "qosReference",
          "flowInfo"
        ],
        "type": "object",
        "properties": {
          "self": {
            "title": "Self",
            "type": "string",
            "description": "Resource URL of the subscription"
          },
          "supportedFeatures": {
            "title": "Supportedfeatures",
            "type": "string",
            "description": "Resource URL of the subscription"
          },
          "qosReference": {
            "title": "Qosreference",
            "type": "string",
            "description": "Reference of the requested QoS level"
          },
          "flowInfo": {
            "title": "Flowinfo",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/FlowInfo"
            },
            "description": "Flow informations that might be affected by the given QoS operation"
          },
          "ueIpv4Addr": {
            "title": "Ueipv4Addr",
            "type": "string",
            "description": "IPV4 address of the device"
          },
          "ueIpv6Addr": {
            "title": "Ueipv6Addr",
            "type": "string",
            "description": "IPV4 address of the device"
          }
        }
      },
      "FlowInfo": {
        "title": "FlowInfo",
        "type": "object",
        "properties": {
          "flowId": {
            "title": "Flowid",
            "type": "integer",
            "description": "ID of the flow"
          },
          "flowDescriptions": {
            "title": "Flowdescriptions",
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Descriptors of the given flow"
          }
        }
      },
      "HTTPValidationError": {
        "title": "HTTPValidationError",
        "type": "object",
        "properties": {
          "detail": {
            "title": "Detail",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            }
          }
        }
      },
      "QoSResource": {
        "title": "QoSResource",
        "required": [
          "qos",
          "id",
          "ip",
          "appIp"
        ],
        "type": "object",
        "properties": {
          "qos": {
            "title": "Qos",
            "type": "string",
            "description": "Name of the requested QoS"
          },
          "id": {
            "title": "Id",
            "type": "string",
            "description": "Identifier of the device"
          },
          "ip": {
            "title": "Ip",
            "type": "string",
            "description": "IP address of the device"
          },
          "ports": {
            "title": "Ports",
            "type": "string",
            "description": "Like 5060-5070, 12345"
          },
          "appIp": {
            "title": "Appip",
            "type": "string",
            "description": "IP address of the application"
          },
          "appPorts": {
            "title": "Appports",
            "type": "string",
            "description": "Like 5060-5070, 12345"
          }
        }
      },
      "ValidationError": {
        "title": "ValidationError",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "type": "object",
        "properties": {
          "loc": {
            "title": "Location",
            "type": "array",
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            }
          },
          "msg": {
            "title": "Message",
            "type": "string"
          },
          "type": {
            "title": "Error Type",
            "type": "string"
          }
        }
      }
    }
  },
  "security": [
    {
       "RapidApiKey": []
    }
  ]
}
