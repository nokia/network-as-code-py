{
  "openapi": "3.0.2",
  "info": {
    "title": "QoS",
    "description": "QoS manages communication bandwidth for a given device.",
    "version": "0.1.0"
  },
  "paths": {
    "/sessions": {
      "get": {
        "summary": "Return QoS settings for your device.",
        "description": "Return device communication bandwidth (QoS) settings for your device.",
        "operationId": "getAllSessions",
        "parameters": [
          {
            "description": "Device ID",
            "required": true,
            "schema": {
              "title": "Device-Id",
              "type": "string",
              "description": "Device ID"
            },
            "name": "device-id",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "title": "Response Getallsessions",
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/SessionInfo"
                  }
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      },
      "post": {
        "summary": "Create QoS service",
        "description": "Create device communication bandwidth (QoS) service.",
        "operationId": "createSession",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateSession"
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SessionInfo"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/sessions/{sessionId}": {
      "get": {
        "summary": "Return QoS settings.",
        "description": "Return device communication bandwidth (QoS) settings.",
        "operationId": "getSession",
        "parameters": [
          {
            "required": true,
            "schema": {
              "title": "Sessionid",
              "type": "string"
            },
            "name": "sessionId",
            "in": "path"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SessionInfo"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      },
      "delete": {
        "summary": "Delete Qos Handler",
        "operationId": "deleteSession",
        "parameters": [
          {
            "required": true,
            "schema": {
              "title": "Sessionid",
              "type": "string"
            },
            "name": "sessionId",
            "in": "path"
          }
        ],
        "responses": {
          "204": {
            "description": "Successful Response"
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "RapidApiKey": {
        "type": "apiKey",
        "in": "header",
        "name": "X-RapidAPI-Key"
      }
    },
    "schemas": {
      "CreateSession": {
        "title": "CreateSession",
        "required": [
          "qosProfile",
          "id",
          "ip",
          "appIp"
        ],
        "type": "object",
        "properties": {
          "qosProfile": {
            "title": "Qosprofile",
            "type": "string",
            "description": "Name of the requested QoS profile."
          },
          "id": {
            "title": "Id",
            "type": "string",
            "description": "Identifier of the device"
          },
          "ip": {
            "title": "Ip",
            "type": "string",
            "description": "IP address of the device"
          },
          "devicePorts": {
            "$ref": "#/components/schemas/PortsSpec"
          },
          "appIp": {
            "title": "Appip",
            "type": "string",
            "description": "IP address of the application"
          },
          "applicationServerPorts": {
            "$ref": "#/components/schemas/PortsSpec"
          },
          "duration": {
            "title": "Duration",
            "maximum": 86400.0,
            "minimum": 1.0,
            "type": "integer",
            "description": "Session duration in seconds."
          },
          "notificationUrl": {
            "title": "Notificationurl",
            "type": "string",
            "description": "Notification URL for session-related events."
          }
        }
      },
      "HTTPValidationError": {
        "title": "HTTPValidationError",
        "type": "object",
        "properties": {
          "detail": {
            "title": "Detail",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            }
          }
        }
      },
      "PortsSpec": {
        "title": "PortsSpec",
        "type": "object",
        "properties": {
          "ranges": {
            "title": "Ranges",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/PortsSpecRangesInner"
            }
          },
          "ports": {
            "title": "Ports",
            "maximum": 65535.0,
            "minimum": 1.0,
            "type": "array",
            "items": {
              "maximum": 65535.0,
              "minimum": 1.0,
              "type": "integer"
            },
            "description": "List of port numbers the application may use. Optional."
          }
        }
      },
      "PortsSpecRangesInner": {
        "title": "PortsSpecRangesInner",
        "required": [
          "from",
          "to"
        ],
        "type": "object",
        "properties": {
          "from": {
            "title": "From",
            "maximum": 65535.0,
            "minimum": 1.0,
            "type": "integer",
            "description": "Start of port range."
          },
          "to": {
            "title": "To",
            "maximum": 65535.0,
            "minimum": 1.0,
            "type": "integer",
            "description": "End of port range."
          }
        }
      },
      "SessionInfo": {
        "title": "SessionInfo",
        "required": [
          "id",
          "qosProfile",
          "qosStatus"
        ],
        "type": "object",
        "properties": {
          "id": {
            "title": "Id",
            "type": "string",
            "description": "ID of the created resource."
          },
          "qosProfile": {
            "title": "Qosprofile",
            "type": "string",
            "description": "Name of the QoS profile."
          },
          "qosStatus": {
            "title": "Qosstatus",
            "type": "string",
            "description": "Status of the QoS resource. REQUESTED, AVAILABLE or UNAVAILABLE"
          },
          "startedAt": {
            "title": "Startedat",
            "type": "integer",
            "description": "Timestamp of session start in seconds since unix epoch",
            "nullable": true
          },
          "expiresAt": {
            "title": "Expiresat",
            "type": "integer",
            "description": "Timestamp of session expiration if the session was not deleted, in seconds since unix epoch",
            "nullable": true
          }
        }
      },
      "ValidationError": {
        "title": "ValidationError",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "type": "object",
        "properties": {
          "loc": {
            "title": "Location",
            "type": "array",
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            }
          },
          "msg": {
            "title": "Message",
            "type": "string"
          },
          "type": {
            "title": "Error Type",
            "type": "string"
          }
        }
      }
    }
  },
  "security": [
    {
       "RapidApiKey": []
    }
  ]
}
